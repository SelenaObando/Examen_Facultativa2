<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageViewList"
        android:layout_width="88dp"
        android:layout_height="92dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toStartOf="@+id/textViewNombres"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/star_big_on" />

    <TextView
        android:id="@+id/textViewNombres"
        android:layout_width="185dp"
        android:layout_height="19dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageViewList"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewApellidos"
        android:layout_width="185dp"
        android:layout_height="24dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageViewList"
        app:layout_constraintTop_toBottomOf="@+id/textViewNombres" />

    <TextView
        android:id="@+id/textViewIdentificacion"
        android:layout_width="187dp"
        android:layout_height="23dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageViewList"
        app:layout_constraintTop_toBottomOf="@+id/textViewApellidos" />


    <TextView
        android:id="@+id/textViewCargo"
        android:layout_width="185dp"
        android:layout_height="21dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageViewList"
        app:layout_constraintTop_toBottomOf="@+id/textViewIdentificacion" />

    <TextView
        android:id="@+id/textViewArea"
        android:layout_width="181dp"
        android:layout_height="23dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageViewList"
        app:layout_constraintTop_toBottomOf="@+id/textViewCargo" />

    <TextView
        android:id="@+id/textViewTelefono"
        android:layout_width="173dp"
        android:layout_height="21dp"
        android:layout_marginTop="32dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageViewList"
        app:layout_constraintTop_toBottomOf="@+id/textViewArea" />

</androidx.constraintlayout.widget.ConstraintLayout>